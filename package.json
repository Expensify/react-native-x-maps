{
  "name": "react-native-x-maps",
  "version": "1.0.3",
  "description": "A cross platform compatible map component for React-Native",
  "main": "./dist/index.js",
  "react-native": "./dist/index.js",
  "browser": "./dist/index.web.js",
  "files": [
    "dist",
    "src"
  ], 
  "scripts": {
    "build": "rm -rf dist && npm run ts:declarations && npx babel src --out-dir dist --extensions '.ts,.tsx'",
    "lint": "eslint src --fix",
    "ts": "npx tsc --noEmit",
    "ts:declarations": "npx tsc --emitDeclarationOnly",
    "prettier": "prettier --write .",
    "prettier-watch": "onchange \"**/*.js\" -- prettier --write --ignore-unknown {{changed}}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Expensify/react-native-x-maps.git"
  },
  "keywords": [
    "React",
    "React-Native",
    "Map"
  ],
  "author": "Expensify, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Expensify/react-native-x-maps/issues"
  },
  "homepage": "https://github.com/Expensify/react-native-x-maps#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/react": "^18.2.18",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
